{
  "name": "kungfu-panda",
  "version": "0.0.1",
  "description": "A test project for applicants to perform some tasks",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf 'dist/!(.*)'",
    "build": "browserify -r dotenv/config -e src/index.js | uglifyjs -c warnings=false -m > dist/build.js",
    "revision": "nf run node ./revision.js",
    "release": "npm run clean && npm run build && npm run revision",
    "watch": "watchify -v -d src/index.js -o dist/build.js",
    "start": "npm run revision && nf start",
    "postinstall": "if [ $BUILD_AFTER_INSTALL ]; then npm run build; fi",
    "test": "echo \"No test specified. Continue anyway\" && exit 0"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "foreman": "^1.4.1",
    "jade": "^1.11.0",
    "throng": "^4.0.0",
    "vue": "^1.0.24"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "browserify": "^13.0.1",
    "dotenv": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-flatten": "^0.3.0",
    "gulp-rev-all": "^0.8.24",
    "less-css-stream": "^1.0.0",
    "rimraf": "^2.5.3",
    "uglify-js": "^2.7.0",
    "vueify": "^8.5.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "vueify"
    ]
  }
}
